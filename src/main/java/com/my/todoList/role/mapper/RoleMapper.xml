<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.my.todoList.role.mapper.RoleMapper">
	<resultMap id="roleMap" type="com.my.todoList.role.Roles">
		<id column="role_no" property="roleNo"/>
		<result column="name" property="name"/>
	</resultMap>
	<resultMap id="userRoleMap" type="com.my.todoList.role.UserRole">
		<id column="userrole_no" property="userroleNo"/>
		<association property="role" javaType="com.my.todoList.role.Roles"
		resultMap="com.my.todoList.role.mapper.RoleMapper.roleMapper"/>
		<association property="user" javaType="com.my.todoList.user.Users"
		resultMap="com.my.todoList.user.mapper.UserMapper.userMap"/>
	</resultMap>
</mapper>